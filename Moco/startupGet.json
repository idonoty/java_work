[
  {
    "description":"一个没有参数的get请求",
      "request":{
        "uri":"/getdemo",
        "method":"get"
      },
      "response":{
        "text":"这是一个无需传参的get请求",
        "headers":{
          "Content-Type":"text/html;charset=gbk"
        }
      }
  },
  {
    "description":"带参数的get请求",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries":{
        "name":"yyc",
        "age":"25"
      }
    },
    "response":{
      "text":"yyc还是18岁",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  }
]